{
    "name": "ToyDatasetOperator",
    "version": "0.1.0",
    "dependency": {
        "docker_base_image": "default",
        "dockerfile": "default",
        "requirements_file": "requirements_1.txt"
    },
    "tags": [
        "SpikeInterface"
    ],
    "style": {
        "node_label": "Create Toy Dataset",
        "target_position": "null",
        "icon_class_name": "fas fa-database"
    },
    "form_schema": {
        "title": "OperatorModel",
        "description": "Toy Dataset",
        "type": "object",
        "properties": {
            "dataset_id": {
                "title": "Dataset Id",
                "description": "Dataset unique ID",
                "default": "toy_dataset",
                "type": "string"
            },
            "duration": {
                "title": "Duration",
                "description": "duration in seconds",
                "default": 10.0,
                "exclusiveMinimum": 0.0,
                "type": "number"
            },
            "num_channels": {
                "title": "Num Channels",
                "description": "number of channels",
                "default": 4,
                "exclusiveMinimum": 0,
                "type": "integer"
            },
            "num_units": {
                "title": "Num Units",
                "description": "number of units",
                "default": 10,
                "exclusiveMinimum": 0,
                "type": "integer"
            },
            "sampling_frequency": {
                "title": "Sampling Frequency",
                "description": "Sampling frequency in Hz",
                "default": 30000.0,
                "exclusiveMinimum": 0.0,
                "type": "number"
            },
            "average_peak_amplitude": {
                "title": "Average Peak Amplitude",
                "description": "Average peak amplitude",
                "default": -100.0,
                "type": "number"
            },
            "upsample_factor": {
                "title": "Upsample Factor",
                "description": "upsample factor",
                "default": 13,
                "exclusiveMinimum": 0,
                "type": "integer"
            },
            "contact_spacing_um": {
                "title": "Contact Spacing Um",
                "default": [
                    40.0
                ],
                "type": "number"
            },
            "num_columns": {
                "title": "Num Columns",
                "default": [
                    1
                ],
                "type": "integer"
            },
            "seed": {
                "title": "Seed",
                "default": [
                    0
                ],
                "type": "integer"
            },
            "include_results_in_report": {
                "title": "Include Results In Report",
                "default": false,
                "type": "boolean"
            }
        }
    }
}