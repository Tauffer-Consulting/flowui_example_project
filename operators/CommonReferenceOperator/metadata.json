{
    "name": "CommonReferenceOperator",
    "version": "0.1.0",
    "dependency": {
        "docker_base_image": "default",
        "dockerfile": "default",
        "requirements_file": "requirements_1.txt"
    },
    "tags": [
        "SpikeInterface"
    ],
    "style": {
        "node_label": "Common reference filter",
        "icon_class_name": "fas fa-filter"
    },
    "input_schema": {
        "title": "OperatorModel",
        "description": "Common reference filter",
        "type": "object",
        "properties": {
            "reference_operator": {
                "default": "average",
                "allOf": [
                    {
                        "$ref": "#/definitions/ReferenceOperatorType"
                    }
                ]
            },
            "include_results_in_report": {
                "title": "Include Results In Report",
                "default": false,
                "type": "boolean"
            }
        },
        "definitions": {
            "ReferenceOperatorType": {
                "title": "ReferenceOperatorType",
                "description": "An enumeration.",
                "enum": [
                    "average",
                    "median"
                ],
                "type": "string"
            }
        }
    }
}