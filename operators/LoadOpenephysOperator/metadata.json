{
    "name": "LoadOpenephysOperator",
    "version": "0.1.0",
    "dependency": {
        "docker_base_image": "default",
        "dockerfile": "default",
        "requirements_file": "requirements_1.txt"
    },
    "tags": [
        "SpikeInterface"
    ],
    "style": {
        "node_label": "Load OpenEphys",
        "icon_class_name": "fas fa-database",
        "target_position": "null",
        "node_style": {
            "backgroundColor": "#b3cde8"
        }
    },
    "input_schema": {
        "title": "OperatorModel",
        "description": "Load Openephys dataset from S3",
        "type": "object",
        "properties": {
            "bucket_name": {
                "title": "Bucket Name",
                "description": "this is the name of the bucket",
                "default": "catalystneuro",
                "type": "string"
            },
            "dataset_path": {
                "title": "Dataset Path",
                "description": "this is the path for the dataset",
                "default": "openephys",
                "type": "string"
            },
            "include_results_in_report": {
                "title": "Include Results In Report",
                "default": true,
                "type": "boolean"
            }
        }
    }
}