{
    "name": "LoadDataFromS3Operator",
    "version": "0.1.0",
    "dependency": {
        "docker_base_image": "default",
        "dockerfile": "default",
        "requirements_file": "requirements_1.txt"
    },
    "tags": [
        "Example"
    ],
    "style": {
        "node_label": "Load Data from S3",
        "target_position": "null",
        "node_style": {
            "backgroundColor": "#b3cde8"
        },
        "icon_class_name": "fas fa-database"
    },
    "input_schema": {
        "title": "OperatorModel",
        "description": "Gets data from S3 path",
        "type": "object",
        "properties": {
            "bucket_name": {
                "title": "Bucket Name",
                "description": "this is the name of the bucket",
                "default": "my-default-bucket",
                "type": "string"
            },
            "dataset_path": {
                "title": "Dataset Path",
                "description": "this is the path for the dataset",
                "default": "my-dataset",
                "type": "string"
            },
            "data_file_type": {
                "default": "csv",
                "allOf": [
                    {
                        "$ref": "#/definitions/DataFileType"
                    }
                ]
            },
            "include_results_in_report": {
                "title": "Include Results In Report",
                "default": false,
                "type": "boolean"
            }
        },
        "definitions": {
            "DataFileType": {
                "title": "DataFileType",
                "description": "whether the data is stored in a CSV or JSON file.",
                "enum": [
                    "csv",
                    "json"
                ],
                "type": "string"
            }
        }
    }
}