{
    "name": "ApplyCV2FilterOperator",
    "version": "0.1.0",
    "dependency": {
        "docker_base_image": "default",
        "dockerfile": "default",
        "requirements_file": "requirements_1.txt"
    },
    "tags": [
        "Example"
    ],
    "style": {
        "node_label": "Apply filter",
        "target_position": "null",
        "node_style": {
            "backgroundColor": "#b3cde8"
        },
        "icon_class_name": "fas fa-database"
    },
    "input_schema": {
        "title": "OperatorModel",
        "description": "Apply effect to image",
        "type": "object",
        "properties": {
            "effect": {
                "description": "Effect to be applied",
                "default": "random",
                "allOf": [
                    {
                        "$ref": "#/definitions/EffectType"
                    }
                ]
            }
        },
        "definitions": {
            "EffectType": {
                "title": "EffectType",
                "description": "An enumeration.",
                "enum": [
                    "random",
                    "grayscale",
                    "bright",
                    "dark",
                    "sharp",
                    "sepia",
                    "pencil",
                    "pencil_color",
                    "hdr",
                    "invert",
                    "summer",
                    "winter"
                ],
                "type": "string"
            }
        }
    }
}