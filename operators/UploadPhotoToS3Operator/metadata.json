{
    "name": "UploadPhotoToS3Operator",
    "version": "0.1.0",
    "dependency": {
        "docker_base_image": "default",
        "dockerfile": "default",
        "requirements_file": "requirements_0.txt"
    },
    "tags": [
        "Example"
    ],
    "style": {
        "node_label": "Upload photo to S3",
        "target_position": "null",
        "node_style": {
            "backgroundColor": "#b3cde8"
        },
        "icon_class_name": "fas fa-database"
    },
    "input_schema": {
        "title": "OperatorModel",
        "description": "Upload photo from mounted volume to S3 folder",
        "type": "object",
        "properties": {
            "s3_bucket_name": {
                "title": "S3 Bucket Name",
                "description": "this is the name of the bucket",
                "default": "bucket-name",
                "type": "string"
            },
            "s3_folder_name": {
                "title": "S3 Folder Name",
                "description": "this is the folder path within the bucket",
                "default": "edited",
                "type": "string"
            }
        }
    }
}