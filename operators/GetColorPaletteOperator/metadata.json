{
    "name": "GetColorPaletteOperator",
    "version": "0.1.0",
    "dependency": {
        "docker_base_image": "default",
        "dockerfile": "default",
        "requirements_file": "requirements_0.txt"
    },
    "tags": [
        "Example"
    ],
    "style": {
        "node_label": "Get color pallete",
        "icon_class_name": "fa-solid fa-palette"
    },
    "form_schema": {
        "title": "OperatorModel",
        "description": "Get color palette from image, using Colormind (http://colormind.io)",
        "type": "object",
        "properties": {
            "model": {
                "default": "default",
                "allOf": [
                    {
                        "$ref": "#/definitions/PaletteModelType"
                    }
                ]
            },
            "include_results_in_report": {
                "title": "Include Results In Report",
                "default": false,
                "type": "boolean"
            }
        },
        "definitions": {
            "PaletteModelType": {
                "title": "PaletteModelType",
                "description": "An enumeration.",
                "enum": [
                    "default",
                    "ui",
                    "makoto_shinkai",
                    "metroid_fusion",
                    "akira_film",
                    "flower_photography"
                ],
                "type": "string"
            }
        }
    }
}